		"@floating-ui/dom": "^1.7.4",
		"@fontsource/inter": "^5.2.8",
		"@khmyznikov/pwa-install": "^0.5.8",
		"@neondatabase/serverless": "^1.0.2",
		"@openrouter/ai-sdk-provider": "^1.4.0",
		"@opentelemetry/api": "^1.9.0",
		"@paralleldrive/cuid2": "^3.0.4",
		"@stacksee/analytics": "^0.13.0",
		"@svelte-put/async-stack": "^1.5.3",
		"@tanstack/svelte-form": "^1.27.1",
		"@tanstack/zod-form-adapter": "^0.42.1",
		"@upstash/redis": "^1.35.7",
		"@upstash/workflow": "^0.2.23",
		"@vercel/functions": "^3.3.4",
		"ai": "^5.0.107",
		"better-auth": "1.4.13",
		"better-auth-ui-svelte": "^0.5.0",
		"better-svelte-email": "^1.1.0",
		"drizzle-orm": "^0.44.7",
		"drizzle-zod": "^0.8.3",
		"expr-eval": "^2.0.2",
		"postgres": "^3.4.7",
		"svelte-tiptap": "^3.0.1",
		"web-push": "^3.6.7",
		"ws": "^8.18.3",
		"yaml": "^2.8.2",
		"zod": "^4.1.13"
	}
}
