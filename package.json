{
	"name": "emitkit",
	"version": "0.1.0",
	"description": "Open-source event streaming platform with real-time notifications, webhooks, and integrations. Self-hostable on any platform.",
	"private": false,
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/emitkithq/emitkit.git"
	},
	"bugs": {
		"url": "https://github.com/emitkithq/emitkit/issues"
	},
	"homepage": "https://github.com/emitkithq/emitkit#readme",
	"keywords": [
		"sveltekit",
		"svelte5",
		"events",
		"notifications",
		"real-time",
		"push-notifications",
		"webhooks",
		"analytics"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm db:migrate && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"docker:up": "docker compose up -d",
		"docker:down": "docker compose down",
		"docker:logs": "docker compose logs -f",
		"docker:restart": "docker compose restart",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"clear-cache": "rm -rf node_modules/.vite && rm -rf .svelte-kit",
		"vite-node": "vite-node --options.transformMode.ssr='/.*/' ",
		"qstash": "pnpm dlx @upstash/qstash-cli dev",
		"openapi:lint": "redocly lint openapi/openapi.yaml",
		"openapi:bundle": "redocly bundle openapi/openapi.yaml -o openapi/openapi.json"
	},
	"devDependencies": {
		"@emoji-mart/data": "^1.2.1",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@playwright/test": "^1.57.0",
		"@redocly/cli": "^2.12.3",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/table-core": "^8.21.3",
		"@types/node": "^22.19.1",
		"@types/web-push": "^3.6.4",
		"@types/ws": "^8.18.1",
		"@vite-pwa/sveltekit": "^1.1.0",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.8",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"formsnap": "^2.0.1",
		"globals": "^16.5.0",
		"layerchart": "2.0.0-next.27",
		"mode-watcher": "^1.1.0",
		"motion": "^12.23.25",
		"paneforge": "^1.0.2",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"runed": "^0.31.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.7",
		"svelte-toolbelt": "^0.9.3",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tinybird": "^0.7.1",
		"tsx": "^4.21.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^7.2.2",
		"vite-node": "^5.2.0",
		"vite-plugin-devtools-json": "^1.0.0",
		"vite-plugin-pwa": "^1.2.0",
		"vitest": "^4.0.15",
		"workbox-window": "^7.4.0"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.77",
		"@bentonow/bento-node-sdk": "^0.2.1",
